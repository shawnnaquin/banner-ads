{
  "name": "run-all-gulps",
  "version": "1.0.0",
  "description": "run all gulpfiles in subdirectory",
  "main": "gulpfile.js",
  "scripts": {
    "cleanremote": "gulp cleanremote",
    "clean": "gulp clean",
    "build": "gulp build",
    "version": "gulp version",
    "zip": "bash zip.sh",
    "rename": "gulp name && bash rename.sh",
    "serve": "gulp serve",
    "ftp": "gulp ftp",
    "ftpIndex": "gulp moveIndex && gulp ftpIndex",
    "all": "npm run clean && npm run build && npm run zip && npm run rename",
    "allftp": "npm run all && npm run cleanremote && npm run ftp",
    "allserve": "npm run all && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "http://level2d.com"
  },
  "keywords": [
    "gulp",
    "workflow",
    "automation"
  ],
