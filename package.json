  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.17.3",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.5.0",
    "gulp-chug": "^0.5.1",
    "gulp-ftp": "^1.1.0",
    "gulp-inject-string": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.7",
    "gulp-vinyl-zip": "^1.4.0",
    "vinyl-ftp": "^0.5.0",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-concat": "^2.6.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-htmlmin": "^3.0.0",
